<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:right="@dimen/timeline_item_stroke_width_negative">
        <shape>
            <stroke
                android:width="@dimen/timeline_item_stroke_width"
                android:color="@color/expense_stroke" />

            <solid 
                android:color="@color/expense_background"/>
            
            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp" />

            <corners
                android:bottomLeftRadius="@dimen/timeline_item_radius"
                android:topLeftRadius="@dimen/timeline_item_radius" />
        </shape>
    </item>

</layer-list>